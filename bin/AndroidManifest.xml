<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sellfunn"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sellfunn.MainActivity"
            android:label="@string/app_name" 
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sellfunn.Contact"
            android:label="@string/title_activity_contact" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.sellfunn.Login"
            android:label="@string/title_activity_login" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.sellfunn.Products"
            android:label="@string/title_activity_products"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.sellfunn.Register"
            android:label="@string/title_activity_register" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.sellfunn.Description"
            android:label="@string/title_activity_description" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.sellfunn.Cart"
            android:label="@string/title_activity_cart" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.sellfunn.Sellfunn"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sellfunn.Dialog"
            android:label="@string/title_activity_dialog" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.sellfunn.Payment"
            android:label="@string/title_activity_payment" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.sellfunn.Books"
            android:label="@string/title_activity_books"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.sellfunn.Robotics"
            android:label="@string/title_activity_robotics"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.sellfunn.Notes"
            android:label="@string/title_activity_notes"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.sellfunn.Others"
            android:label="@string/title_activity_others"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.sellfunn.Profile"
            android:label="@string/title_activity_profile"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.sellfunn.Upload"
            android:label="@string/title_activity_upload" 
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
